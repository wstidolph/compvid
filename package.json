    "kill-all": "npx kill-port 4000 5000 5001 8080 8085 9099 9199",
    "prepare.ionic.ruskinv.ios": "cd apps/rusk/ionic-ruskinv && npm run cap.add.ios",
    "prepare.ionic.ruskinv.android": "cd apps/rusk/ionic-ruskinv && npm run cap.add.android",

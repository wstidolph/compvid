  "scripts": {
    "start": "nx serve",
    "build": "nx build",
    "test": "nx test",
    "clean": "npx rimraf hooks node_modules package-lock.json yarn.lock && yarn",
    "prepare.ionic.compariview.ios": "cd apps/ionic-compariview && npm run cap.add.ios",
    "prepare.ionic.compariview.android": "cd apps/ionic-compariview && npm run cap.add.android",
    "open.ionic.compariview.ios": "cd apps/ionic-compariview && npm run cap.ios",
    "open.ionic.compariview.android": "cd apps/ionic-compariview && npm run cap.android",
    "sync.ionic.compariview": "cd apps/ionic-compariview && npm run cap.sync",
    "clean.ionic.compariview": "cd apps/ionic-compariview && npx rimraf hooks node_modules platforms www plugins package-lock.json && yarn config set ignore-engines true && yarn",
    "postinstall": "patch-package && ngcc --properties es2015 browser module main",
    "kill-all": "npx kill-port 4000 5000 5001 8080 8085 9099 9199",
    "prepare.ionic.ruskinv.ios": "cd apps/rusk/ionic-ruskinv && npm run cap.add.ios",
    "prepare.ionic.ruskinv.android": "cd apps/rusk/ionic-ruskinv && npm run cap.add.android",
    "open.ionic.ruskinv.ios": "cd apps/rusk/ionic-ruskinv && npm run cap.ios",
    "open.ionic.ruskinv.android": "cd apps/rusk/ionic-ruskinv && npm run cap.android",
    "sync.ionic.ruskinv": "cd apps/rusk/ionic-ruskinv && npm run cap.sync",
    "clean.ionic.ruskinv": "cd apps/rusk/ionic-ruskinv && npx rimraf hooks node_modules platforms www plugins package-lock.json yarn.lock && yarn"
  },

    "sync.ionic.compariview": "cd apps/ionic-compariview && npm run cap.sync",
    "clean.ionic.compariview": "cd apps/ionic-compariview && npx rimraf hooks node_modules platforms www plugins package-lock.json && yarn config set ignore-engines true && yarn",
